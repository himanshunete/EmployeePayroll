<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    <link rel="stylesheet" href="~/Content/employee_form.css" type="text/css" />
    <link rel="stylesheet" href="~/Content/display.css" type="text/css" />
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <header class="topnav">
        <img class="image-title" src="~/assets/employee_payroll.PNG" />
        <div class="title">
            <div class="title1">EMPLOYEE</div>
            <div class="title2">PAYROLL</div>
        </div>
    </header>
        
    
    <div class="container-form">
        @RenderBody()
        <hr />
        <footer>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
